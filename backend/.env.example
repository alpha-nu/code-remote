# Application
DEBUG=true
ENVIRONMENT=development

# Server
HOST=0.0.0.0
PORT=8000

# CORS (comma-separated origins)
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Execution limits
EXECUTION_TIMEOUT_SECONDS=30
MAX_CODE_SIZE_BYTES=10240

# Redis
REDIS_URL=redis://localhost:6379

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/coderemote

# Gemini LLM (get key from https://aistudio.google.com/app/apikey)
GEMINI_API_KEY=
