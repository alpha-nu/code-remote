# Frontend Environment Variables
# Copy to .env.local and fill in values

# Backend API URL
VITE_API_URL=

# WebSocket URL for real-time execution updates
# Local: ws://localhost:8000/ws
# Production: wss://xxx.execute-api.region.amazonaws.com/stage
VITE_WEBSOCKET_URL=

# AWS Cognito Configuration
VITE_COGNITO_USER_POOL_ID=
VITE_COGNITO_CLIENT_ID=
VITE_COGNITO_DOMAIN=
VITE_COGNITO_REDIRECT_SIGNIN=
VITE_COGNITO_REDIRECT_SIGNOUT=

# Development Settings
# WARNING: NEVER enable in production!
# VITE_DEV_AUTH_BYPASS=

# Snippet auto-save interval (seconds)
# Auto-save triggers after successful execution or after this interval
VITE_SNIPPET_AUTOSAVE_INTERVAL=30
